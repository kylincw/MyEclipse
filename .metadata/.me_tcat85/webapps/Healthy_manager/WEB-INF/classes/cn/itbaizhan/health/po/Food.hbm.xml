<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.itbaizhan.health.po.Food">
		<id name="id" type="java.lang.Integer">
			<generator class="native" />
		</id>
		<property name="name" type="java.lang.String" not-null="true" length="20"/>
		<property name="heat" type="java.lang.Double" />
		<property name="carbohydrate" type="java.lang.Double" />
		<property name="fat" type="java.lang.Double" />
		<property name="protein" type="java.lang.Double" />
		<property name="fiber" type="java.lang.Double" />
		<property name="type" type="java.lang.Integer" not-null="true" length="2" />

	</class>
</hibernate-mapping>
